{
  "name": "soulvan-racing",
  "version": "0.1.0",
  "description": "Soulvan Racing - ultra realistic 16K global driving simulator",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "electron .",
    "test": "jest",
    "install-global": "npm install -g .",
    "dev": "node --experimental-modules",
    "faction": "node src/cli/factionforge.js",
    "war": "node src/cli/warforge.js",
    "mythbook": "node src/cli/mythbook.js",
    "treasury": "node src/cli/treasuryvote.js"
  },
  "bin": {
    "factionforge": "./src/cli/factionforge.js",
    "warforge": "./src/cli/warforge.js",
    "mythbook": "./src/cli/mythbook.js",
    "treasuryvote": "./src/cli/treasuryvote.js"
  },
  "dependencies": {
    "electron": "^28.0.0",
    "cesium": "^1.120.0",
    "mapbox-gl": "^3.0.0",
    "dlss-sdk": "latest",
    "fsr-sdk": "latest",
    "ethers": "^6.8.0",
    "chalk": "^5.3.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "@types/node": "^20.5.0",
    "webpack": "^5.90.0",
    "jest": "^29.0.0"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "gaming",
    "racing",
    "factions",
    "cli",
    "web3",
    "smart-contracts"
  ],
  "author": "SoulvanRacing Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/44547/Brian"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "preferGlobal": true
}